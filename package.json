{
  "name": "vue-docs",
  "version": "1.1.7",
  "description": "vue docs",
  "scripts": {
    "prepublish": "npm run compile",
    "copy": "rm -fr dist && cp -rf src dist",
    "compile": "npm run copy && node_modules/.bin/babel src -d dist"
  },
  "bin": {
    "vue-docs": "./bin/vue-docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-tools/vue-docs.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vue-tools/vue-docs/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-vue-jsx",
      "transform-runtime"
    ]
  },
  "homepage": "https://github.com/vue-tools/vue-docs#readme",
  "dependencies": {
    "autoprefixer": "^6.7.5",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.9.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-plugin-transform-vue-jsx": "^3.3.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "chokidar": "^1.6.1",
    "css-loader": "^0.23.1",
    "dom-parser": "^0.1.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "highlight.js": "^9.10.0",
    "html-webpack-plugin": "^2.19.0",
    "koa": "^1.1.0",
    "koa-webpack-dev-middleware": "^1.2.1",
    "koa-webpack-hot-middleware": "^1.0.3",
    "log4js": "^1.1.1",
    "marked": "^0.3.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vt-tabs": "^1.0.5",
    "vue": "^2.0.0",
    "vue-hot-reload-api": "^2.0.6",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^9.7.0",
    "vue-router": "^2.0.0",
    "vue-style-loader": "^1.0.0",
    "vuikit-docs": "^0.1.1",
    "webpack": "^1.13.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-merge": "^4.1.0",
    "yamljs": "^0.2.8"
  },
  "devDependencies": {}
}
